{
  "name": "model-scout-mcp",
  "version": "0.2.0",
  "description": "MCP server for discovering, comparing, and selecting LLM models across providers with real-time pricing and capabilities",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "model-scout-mcp": "dist/index.js"
  },
  "scripts": {
    "start": "node dist/index.js",
    "start:http": "MCP_TRANSPORT=http node dist/index.js",
    "dev": "node --watch index.js",
    "dev:http": "MCP_TRANSPORT=http node --watch index.js",
    "build": "mkdir -p dist && cp index.js dist/index.js",
    "prepublishOnly": "npm run build",
    "test": "node test-server.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "llm",
    "openrouter",
    "model-comparison",
    "pricing",
    "cost-estimation",
    "ai",
    "claude",
    "mcp-server"
  ],
  "author": {
    "name": "Daniel Rosehill",
    "email": "public@danielrosehill.com",
    "url": "https://danielrosehill.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/danielrosehill/Model-Scout-MCP.git"
  },
  "bugs": {
    "url": "https://github.com/danielrosehill/Model-Scout-MCP/issues"
  },
  "homepage": "https://github.com/danielrosehill/Model-Scout-MCP#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.1",
    "dotenv": "^16.4.0",
    "express": "^4.21.0"
  },
  "devDependencies": {},
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
